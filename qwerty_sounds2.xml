<Keyboard>
  <Name>qwerty</Name>
  <WindowState>Floating</WindowState>
  <Position>Bottom</Position>
  <Width>95%</Width>
  <!-- <VerticalOffset>-5%</VerticalOffset> -->
  <SymbolMargin>0</SymbolMargin>
  <BorderThickness>0,0,0,20</BorderThickness>
  <BackgroundColor>Transparent</BackgroundColor>
  <HideFromKeyboardMenu>false</HideFromKeyboardMenu>

  <BorderColor>Transparent</BorderColor>
  <Height>85%</Height>
  <Grid>
      <Rows>8</Rows>
      <Cols>12</Cols>
  </Grid>
  

  <KeyGroup Name="abc" BackgroundColor="#E18676" ForegroundColor="#1C1D1E" AutoScaleToOneKeyWidth="false"/>
  <KeyGroup Name="abc_tdsnap" BackgroundColor="#" AutoScaleToOneKeyWidth="false"/>
  <KeyGroup Name="abc_vowels" BackgroundColor="#ACDFE1" ForegroundColor="#1C1D1E" AutoScaleToOneKeyWidth="false"/>
  <KeyGroup Name="123"  ForegroundColor="#1C1D1E" BackgroundColor="#7070aa" AutoScaleToOneKeyWidth="false"/>
    
  <KeyGroup Name="longvowels"    BackgroundColor="#A1DC7A" ForegroundColor="#1C1D1E" AutoScaleToOneKeyWidth="false"/>
  <KeyGroup Name="shortvowels"    BackgroundColor="#ACDFE1" ForegroundColor="#1C1D1E" AutoScaleToOneKeyWidth="false"/>
  <KeyGroup Name="diphthongs"    BackgroundColor="#fcd668" ForegroundColor="#1C1D1E" AutoScaleToOneKeyWidth="false"/>
  <KeyGroup Name="consonants" BackgroundColor="#E18676" ForegroundColor="#1C1D1E" AutoScaleToOneKeyWidth="false"/>
  <KeyGroup Name="consonants2" BackgroundColor="#C05D54" ForegroundColor="#1C1D1E" AutoScaleToOneKeyWidth="false"/>
  
    <!-- 
  <KeyGroup Name="vowels" BackgroundColor="#907070" AutoScaleToOneKeyWidth="false"/>
  <KeyGroup Name="diphthongs" BackgroundColor="#7070aa" AutoScaleToOneKeyWidth="false"/>
  <KeyGroup Name="consonants" BackgroundColor="#007000" AutoScaleToOneKeyWidth="false"/>
  
  <KeyGroup Name="vowels2" BackgroundColor="#ba9090" AutoScaleToOneKeyWidth="false"/>
  <KeyGroup Name="diphthongs" BackgroundColor="#7070aa" AutoScaleToOneKeyWidth="false"/>
  <KeyGroup Name="consonants2" BackgroundColor="#003500" AutoScaleToOneKeyWidth="false"/>
   -->

  <KeyGroup Name="symbols" BackgroundColor="#709070" AutoScaleToOneKeyWidth="false"/>
  <KeyGroup Name="modifiers" AutoScaleToOneKeyWidth="false"/>
  
  <KeyGroup Name="functions1" BackgroundColor="#D69002" AutoScaleToOneKeyWidth="false"/>
  <KeyGroup Name="functions2" BackgroundColor="#D6AA1A" AutoScaleToOneKeyWidth="false"/>
  
  <KeyGroup Name="arrows" BackgroundColor="#907090" AutoScaleToOneKeyWidth="false"/>

  <Content>

    <DynamicKey Width="6" AutoScaleToOneKeyWidth="false">
        <Label>Sounding out</Label>
    </DynamicKey>

    <DynamicKey Width="3" AutoScaleToOneKeyWidth="false">
      <KeyGroup>functions1</KeyGroup>
      <Action>TypeLastPhoneme</Action>
      <Action>Pronounce</Action>
      <Label>Type last</Label>
    </DynamicKey>    

    <DynamicKey Width="3" AutoScaleToOneKeyWidth="false">
      <KeyGroup>functions2</KeyGroup>      
      <Label>Type many</Label>      
      <ChangeKeyboard BackReturnsHere="false">qwerty_type2</ChangeKeyboard>
    </DynamicKey>    


    <DynamicKey Row="1">
      <Label>clear</Label>
      <Action>ClearScratchpad</Action>
      <Symbol>ClearIcon</Symbol>      
    </DynamicKey>
    

    <Scratchpad Row="1" Width="8"></Scratchpad>    


    <DynamicKey>
      <!-- <Label>Back</Label> -->
      <Action>BackOne</Action>
      <Symbol>BackOneIcon</Symbol>
    </DynamicKey>

      <!-- <Label>speak</Label> -->
    <DynamicKey Width="2">
      <Action>Pronounce</Action>
      <Symbol>SpeakIcon</Symbol>
    </DynamicKey>

    
    <DynamicKey Row="2" Width="2"></DynamicKey>
    
    <DynamicKey >
        <KeyGroup>longvowels</KeyGroup>
        <!-- <Label>ee</Label>  -->
        <Symbol>images/sea.png</Symbol>
        <PronounceSlow>iː</PronounceSlow>      
    </DynamicKey>


    <DynamicKey>
        <KeyGroup>diphthongs</KeyGroup>
        <!-- <Label>ear</Label> -->
        <Symbol>images/deer.jpg</Symbol>
        <Pronounce>ɪə</Pronounce>      
    </DynamicKey>

    <DynamicKey>
        <KeyGroup>diphthongs</KeyGroup>
        <!-- <Label>pure</Label> -->
        <Symbol>images/manure.jpg</Symbol>
        <Pronounce>ʊə</Pronounce>      
    </DynamicKey>  

    <DynamicKey></DynamicKey>

    <DynamicKey>
        <KeyGroup>shortvowels</KeyGroup>
        <Symbol>images/the.png</Symbol>
        <Pronounce>ə</Pronounce>      
    </DynamicKey>

    <DynamicKey>
        <KeyGroup>diphthongs</KeyGroup>
        <!-- <Label>music</Label> -->
        <Symbol>images/music.png</Symbol>
        <Pronounce>ɪʊ</Pronounce>      
    </DynamicKey>

    <DynamicKey>
        <KeyGroup>diphthongs</KeyGroup>
        <!-- <Label>oy</Label> -->
        <Symbol>images/boy.png</Symbol>
        <Pronounce>ɔɪ</Pronounce>      
    </DynamicKey>
    
    <DynamicKey>
        <KeyGroup>diphthongs</KeyGroup>
        <!-- <Label>oh</Label> -->
        <Symbol>images/bow.png</Symbol>
        <Pronounce>əʊ</Pronounce>      
    </DynamicKey>


    <DynamicKey>
        <KeyGroup>diphthongs</KeyGroup> 
        <!-- <Label>ow</Label> -->
        <Symbol>images/out.png</Symbol>
        <Pronounce>aʊ</Pronounce>      
    </DynamicKey>
 
    <DynamicKey>
        <KeyGroup>longvowels</KeyGroup>
        <!-- <Label>oo</Label> -->
        <Symbol>images/two.png</Symbol>
        <PronounceSlow>uː</PronounceSlow>      
    </DynamicKey>




    
